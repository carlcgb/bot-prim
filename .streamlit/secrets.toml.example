# Example secrets file for local development
# Copy this to .streamlit/secrets.toml and add your actual keys
# DO NOT commit .streamlit/secrets.toml to git!

# OpenAI API Key (default provider)
OPENAI_API_KEY = "your_openai_api_key_here"

# Optional: Gemini API Key (if using Google Gemini provider)
# GEMINI_API_KEY = "your_gemini_api_key_here"

# Qdrant Cloud Configuration (optional, for cloud knowledge base)
# [qdrant]
# USE_QDRANT = "true"
# QDRANT_URL = "https://your-cluster.qdrant.io:6333"
# QDRANT_API_KEY = "your_qdrant_api_key_here"

